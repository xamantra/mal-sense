{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jquery": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-extend-native": "off",
        "space-before-function-paren": "off",
        "no-async-promise-executor": "off"
    }
}